  rdfs:isDefinedBy <http://terminology.hl7.org/CodeSystem/v3-NullFlavor#UNC> .

<https://w3id.org/fair-genomes/ontology/Studies_Inclusion_criteria_Masked_(MSK,_nullflavor)>
  a obo:OBI_0500027;
  rdfs:label "Masked (MSK, nullflavor)";
