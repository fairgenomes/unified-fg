  rdfs:isDefinedBy <http://terminology.hl7.org/CodeSystem/v3-NullFlavor#MSK> .

<https://w3id.org/fair-genomes/ontology/Studies_Inclusion_criteria_Not_applicable_(NA,_nullflavor)>
  a obo:OBI_0500027;
  rdfs:label "Not applicable (NA, nullflavor)";
